{
  "extends": [
    "next/core-web-vitals"
  ],
  "rules": {
    "react-hooks/exhaustive-deps": "warn",
    "react/no-unescaped-entities": "off",
    "prefer-const": "warn",
    "no-var": "warn",
    "no-console": "warn"
  },
  "ignorePatterns": [
    "node_modules/",
    ".next/",
    "out/",
    "*.config.js",
    "*.config.ts"
  ]
}
